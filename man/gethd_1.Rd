## The function is currently defined as
function (A, string, number = TRUE, position = FALSE) 
{
    if (position) {
        which((trim(A[, 1]) == string) == TRUE)[1]
    }
    else {
        if (number) {
            as.numeric(A[trim(A[, 1]) == string, 2])
        }
        else {
            A[trim(A[, 1]) == string, 2]
        }
    }
  }
}
